##### Templates for student's  files-view #####

<studentfiles> a r2h:Matcher ;
  r2h:triplePattern [
    r2h:subject r2h:this ;
    r2h:predicate s:file
  ];
  r2h:template [
    r2h:context s:Fileview;
    r2h:mustache '''
        {{#s:file}}
            {{{:render .}}}
        {{/s:file}}
      '''
  ] ;
  r2h:before <no_studentfiles> .

<studentfile> a r2h:Matcher ;
  r2h:triplePattern [
    r2h:subject r2h:this;
    r2h:predicate s:fileType;
  ];
  r2h:template [
    r2h:context s:Fileview;
    r2h:mustache '''
         <tr>
            <td><a href="/files/download/student/{{s:fileHandle}}">{{s:fileName}}</a></td>
            <td>{{s:fileSize}}</td>
            <td>{{s:fileType}}</td>
            <td><a href="#/files/view" onclick="if(confirm('Datei &quot;{{s:fileName}}&quot; wirklich löschen?')) { window.deleteFile('{{s:fileHandle}}'); }"><span class="glyphicon glyphicon-trash" aria-hidden="true"></span></a></td>
          </tr>
     '''
  ] ;
  r2h:before <no_studentfiles> .

<no_studentfiles> a r2h:Matcher ;
  r2h:template [
    r2h:context s:Fileview;
    r2h:mustache '''<tr><td colspan="4">Keine Dateien</td></tr>'''
  ] .

##### Templates for student's decision view #####

<student_decision> a r2h:Matcher ;
  r2h:triplePattern [
    r2h:subject r2h:this;
    r2h:predicate rdf:type ;
    r2h:object m:HMSC;
  ];
  r2h:template [
    r2h:context s:Decisionview ;
    r2h:javaScript '''function(n) {
                if(n.graphNode.out(RDF2h.resolveCurie('m:decision')).toString() === 'true') {
                    return '<div class="alert alert-success" role="alert"><span class="glyphicon glyphicon-education" aria-hidden="true"></span>&nbsp;Sie wurden zum Masterstudium zugelassen!</div>';
                } else if (n.graphNode.out(RDF2h.resolveCurie('m:decision')).toString() === 'false') {
                    return '<div class="alert alert-danger" role="alert"><span class="glyphicon glyphicon-ban-circle" aria-hidden="true"></span>&nbsp;Diesmal hat es leider nicht geklappt...!</div>';
                }
            }'''
  ] ;
  r2h:before <no_student_decision> .

<no_student_decision> a r2h:Matcher ;
  r2h:template [
    r2h:context s:Decisionview ;
    r2h:mustache '''<form id="form-decision-upload" action="/import/student" enctype="multipart/form-data" method="post">
          <div class="form-group">
            <label for="in-decision-file">Immatrikulationsbescheid importieren</label>
            <input id="in-decision-file" required type="file" name="upload">
          </div>
          <button id="btn-hmsc-decision-save" type="submit" class="btn btn-default">Importieren&nbsp;<span class="glyphicon glyphicon-ok" aria-hidden="true"/></span></button>
          <button id="btn-hmsc-decision-cancel" type="reset" class="btn btn-default">Abbrechen&nbsp;<span class="glyphicon glyphicon-remove" aria-hidden="true"></span></button>
        </form>'''
  ] .

##### Templates for student's permission view #####

<student_permissions> a r2h:Matcher ;
  r2h:triplePattern [
    r2h:subject r2h:this ;
    r2h:predicate s:permission ;
  ] ;
  r2h:template [
    r2h:context s:Permissionview ;
    r2h:mustache '''<div class="alert alert-info" role="alert"><span class="glyphicon glyphicon-info-sign" aria-hidden="true"></span>&nbsp;Berechtigte WebID: <a href="{{s:permission}}">{{s:permission}}</div>'''
  ] ;
  r2h:before <no_student_permissions> .

<no_student_permissions> a r2h:Matcher ;
  r2h:template [
    r2h:context s:Permissionview ;
    r2h:mustache '''<div class="alert alert-info" role="alert"><span class="glyphicon glyphicon-info-sign" aria-hidden="true"></span>&nbsp;Keine Berechtigung vergeben.</div>'''
  ] .


